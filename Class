
public class ZipCode 
{
	private int zipCode;
	public ZipCode(int z)
	{
		this.zipCode=z;
	}
	
	public String toString()
	{
		String string=""+zipCode;
		while(string.length()<5)
		{
			string="0"+string;
		}
		return string;
	}

	public int getCheckDigit() 
	{
		String string=""+zipCode;
		while(string.length()<5)
		{
			string="0"+string;
		}
		int sum=0;
		for(int i=0;i<string.length();i++)
		{
			sum+=(int)(string.charAt(i));
		}
		return -1;
	}
}
